<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="title" format="string"/>
        <attr name="backEnable" format="boolean"/>
        <attr name="sureEnable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ScaleView">
        <attr name="innerRadius" format="dimension"/>
        <attr name="outRadius" format="dimension"/>
        <attr name="scale" format="integer"/>
        <attr name="numberTextSize" format="dimension"/>
        <attr name="similarityTextSize" format="dimension"/>
        <attr name="textEnable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CameraDecor">
        <attr name="cornerWidth" format="dimension"/>
        <attr name="cornerHeight" format="dimension"/>
        <attr name="outLineStrokeWidth" format="dimension"/>
        <attr name="scanDrawable" format="reference"/>
        <attr name="scanHeight" format="dimension"/>
        <attr name="scanSpeed" format="dimension"/>
        <attr name="scanDelay" format="integer"/>
    </declare-styleable>
</resources>